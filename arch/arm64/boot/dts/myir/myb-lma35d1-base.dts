};

&i2c1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	sgtl5000: audio-codec@a {
		compatible = "fsl,sgtl5000";
		reg = <0xa>;
		#sound-dai-cells = <0>;
		clocks = <&sgtl5000_mclk>;
		VDDA-supply = <&reg_3p3v>;
		VDDIO-supply = <&reg_3p3v>;
		VDDD-supply = <&reg_1p8v>;
		status = "okay";
	};

#if (OV2659_SUPPORT)
	ov2659: camera@30 {
		compatible = "ovti,ov2659";
		reg = <0x30>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ccap0>;
		clocks = <&clk CCAP0_GATE>;
		clock-names = "xvclk";
		powerdown-gpios = <&gpiom 10 (GPIO_ACTIVE_HIGH | GPIO_PUSH_PULL)>;
		reset-gpios = <&gpiom 11 (GPIO_ACTIVE_LOW | GPIO_PUSH_PULL)>;
		status = "okay";
		port {
			ov2659_0: endpoint {
				remote-endpoint = <&ccap0_1>;
				bus-width = <8>;
				data-shift = <2>; /* lines 9:2 are used */
				hsync-active = <1>;
				vsync-active = <0>;
				pclk-sample = <1>;
				link-frequencies = /bits/ 64 <70000000>;
				pclk-max-frequency = <12000000>;
			};
		};
	};
#else
	ov5640: camera@3c {
		compatible = "ovti,ov5640";
		reg = <0x3c>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ccap0>;
		clocks = <&clk CCAP0_GATE>;
		clock-names = "xclk";
		// powerdown-gpios = <&gpiom 10 (GPIO_ACTIVE_HIGH | GPIO_PUSH_PULL)>;
		// reset-gpios = <&gpiom 11 (GPIO_ACTIVE_LOW | GPIO_PUSH_PULL)>;

		powerdown-gpios = <&gpiom 3 (GPIO_ACTIVE_HIGH | GPIO_PUSH_PULL)>;
		reset-gpios = <&gpiom 2 (GPIO_ACTIVE_LOW | GPIO_PUSH_PULL)>;

		status = "okay";
		port {
			ov5640_0: endpoint {
				remote-endpoint = <&ccap0_1>;
				hsync-active = <0>;     /* Active low */
				vsync-active = <0>;     /* Active low */
				pclk-sample = <1>;      /* Rising */
				link-frequencies = /bits/ 64 <70000000>;
			};
		};
	};
#endif

   ft5x06: ft5x06@38 {
        compatible = "edt,edt-ft5406";
        reg = <0x38>;
        interrupt-parent = <&gpiol>;
        interrupts = <14 2>;
        reset-gpios = <&gpiok 0 GPIO_ACTIVE_LOW>;
        touchscreen-size-x=<800>;
        touchscreen-size-y=<480>;
   };

};

