/dts-v1/;

#include "ma35d1.dtsi"


#define OV2659_SUPPORT           0

/ {
	model = "MYIR MA35D1";

	chosen {
		bootargs = "root=/dev/mmcblk0p1 rootfstype=ext4 rw rootwait console=ttyS0,115200n8 rdinit=/sbin/init mem=256M";
	};

	reserved-memory {
		display_buf: display_buf@0 {
			reg = <0x0 0x8C800000 0x0 0x3000000>; /* 48MiB */
			no-map;
		};
		vc8k_buf: vc8k_buf@0 {
			reg = <0x0 0x8A800000 0x0 0x2000000>; /* 32MiB */
			no-map;
		};
		gc520l_buf: gc520l_buf@0 {
			reg = <0x0 0x8A000000 0x0 0x800000>; /* 8MiB */
			no-map;
		};
		rproc_buf: rproc_buf@0 {
			reg = <0x0 0x80020000 0x0 0x60000>; /* 384KB */
			no-map;
		};
	};

	regulators {
			compatible = "simple-bus";
			#address-cells = <1>;
			#size-cells = <0>;

		reg_m2_power_pad: regulator1 {
			compatible = "regulator-fixed";
			regulator-name = "fixed-3.3V";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-boot-on;
			regulator-always-on;
			enable-active-high;
			gpios = <&gpiob 8 GPIO_ACTIVE_HIGH>;
		};

	};

	reg_3p3v: regulator1 {
		compatible = "regulator-fixed";
		regulator-name = "fixed-3.3V";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		regulator-always-on;
	};

	reg_1p8v: regulator0 {
		compatible = "regulator-fixed";
		regulator-name = "fixed-1.8V";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-boot-on;
		regulator-always-on;
	};

	watchdog: watchdog {
		// SGM823
		compatible = "linux,wdt-gpio";
		gpios = <&gpiol 13 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		// always-running;
		hw_margin_ms = <1600>;
	};

	gpio_keys_test {
		compatible = "gpio-keys";
		status = "okay";
		autorepeat;
		botton0 {
			label = "user key";
			linux,code = <KEY_DOWN>;
			gpios = <&gpiok 7 GPIO_ACTIVE_LOW>;
			wakeup-source;
		};
	};

	gpio_leds_test {
		compatible = "gpio-leds";
		status = "okay";
		led0 {
			label = "LED-Blue";
			gpios = <&gpiol 15 GPIO_ACTIVE_LOW>;
			default-state = "on";
			linux,default-trigger = "heartbeat";
		};
	};

	panel_powr1: regulator-panel_powr1 {
		compatible = "regulator-fixed";
		regulator-name = "panel-power1";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpios = <&gpioa 14 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
	};

	panel_powr2: regulator-panel_powr2 {
		compatible = "regulator-fixed";
		regulator-name = "panel-power2";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpios = <&gpiog 14 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
		regulator-always-on;
	};

	backlight: backlight {
		compatible = "pwm-backlight";
		brightness-levels = <250 225 175 125 75 25>;
		default-brightness-level = <5>;
		pwms = <&epwm1 1 50000>;
		enable-gpios = <&gpiok 8 GPIO_ACTIVE_HIGH>;
		status = "okay";
    };

	volt0_sdhci1: regulator-volt0_sdhci1 {
		compatible = "nuvoton,ma35d1-volt";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
		regulator-name = "volt0_sdhci1";
		regulator-always-on;
    };

	sgtl5000_mclk: sgtl5000-mclk {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <11289600>;
	};

};

&mem {
	device_type = "memory";
	reg = <0x00000000 0x80000000 0 0x10000000>; /* DRAM space - 1, size : 256MB DRAM */
};

&ccap0 {
	status = "okay";
	port {
			/* Parallel bus endpoint */
			ccap0_1: endpoint {
#if (OV2659_SUPPORT)
					remote-endpoint = <&ov2659_0>;
#else
					remote-endpoint = <&ov5640_0>;
#endif
					bus-width = <8>;
					hsync-active = <0>;     /* Active low */
					vsync-active = <0>;     /* Active high */
					pclk-sample = <1>;      /* Rising */
			};
	};
};

&i2c3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;

	rx8025t: rx8025t@32 {
		status = "okay";
		compatible = "epson,rx8025t";
		reg = <0x32>;
    };

	EEPROM:24C32F@50 {
		status = "okay";
		compatible = "atmel,24c32";
		reg = <0x50>;
		pagesize = <64>;
	};

	ptn5150: ptn5150@3d {
		status = "okay";
		compatible = "nxp,ptn5150";
		reg = <0x3d>;
		pinctrl-names = "default";
		int-gpios = <&gpiok 5 GPIO_ACTIVE_HIGH>;

		connector {
			compatible = "usb-c-connector";
			label = "USB-C";
			port {
				con_usbotg_hs_ep: endpoint {
					remote-endpoint = <&usbotg_hs_ep>;
				};
			};
		};
	};
};

&ccap1 {
        status = "disabled";
};

&i2c4 {
	status = "disabled";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c4>;
};

&gmac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gmac0>;
	phy-reset-duration = <20>;
	phy-reset-post-delay=<20>;
	phy-reset-gpios = <&gpiof 14 GPIO_ACTIVE_HIGH>;
};

&gmac1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gmac1>;
	phy-reset-duration = <20>;
	phy-reset-post-delay=<20>;
	phy-reset-gpios = <&gpiol 5 GPIO_ACTIVE_HIGH>;
};

&ehci0 {
	// pinctrl-names = "default";
	// pinctrl-0 = <&pinctrl_hsusbh>;
	oc-active-level = <1>;
	status = "okay";
};

&usb {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usb>;
	dr_mode = "otg";
	port {
		usbotg_hs_ep: endpoint {
			remote-endpoint = <&con_usbotg_hs_ep>;
		};
	};
};

&ehci1 {
   status = "okay";
   oc-active-level = <1>;
};

&nand {
	status = "disabled";
};

/* SD */
&sdhci0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_sdhci0>;
	wp-gpios = <&gpioc 7 GPIO_ACTIVE_LOW>;
};

/*eMMC*/
&sdhci1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_sdhci1_3_3V>;
	max-frequency = <200000000>;
	bus-width = <8>;
	no-1-8-v;
	non-removable;
};

&qspi0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_qspi0>;
};

